node {
  stage('SCM') {
    git 'https://github.com/shiwanee-code/SimpleCustomerApp/blob/master/SimpleCustomerApp/'
  }
  stage('SonarQube analysis') {
    def scannerHome = tool 'SonarScanner 4.5.0';
    withSonarQubeEnv('My SonarQube Server') {
      sh "${scannerHome}/opt/sonar-scanner-4.5.0.2216-linux/"
    }
  }
}
